            <TextField fx:id="password" layoutX="-110.0" layoutY="55.0" promptText="password" />
            <TextField fx:id="name" layoutX="203.0" layoutY="29.0" promptText="name" />
            <TextField fx:id="lastname" layoutX="203.0" layoutY="55.0" promptText="lastname" />
            <TextField fx:id="email" layoutX="203.0" layoutY="81.0" promptText="email" />
            <TextField fx:id="phone" layoutX="-110.0" layoutY="81.0" promptText="phone number" />
            <TextField fx:id="age" layoutX="203.0" layoutY="107.0" promptText="age" />
            <TextField fx:id="business" layoutX="-110.0" layoutY="107.0" promptText="Business" />
            <TextField fx:id="answer" layoutX="-110.0" layoutY="133.0" promptText="favorite food" />
         </children></AnchorPane>
   </center>
</BorderPane>
